}

@ARTICLE{Clifton1989-hh,
  title    = "A behavioural profile of fluoxetine-induced anorexia",
  author   = "Clifton, P G and Barnfield, A M and Philcox, L",
  abstract = "Fluoxetine is a specific and long-lasting inhibitor of serotonin
              reuptake. In free-feeding rats a dose of 10 mg/kg reduced meal
