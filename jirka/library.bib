@ARTICLE{Dambach2004-mp,
  title    = "{ToF-SIMS} and {laser-SNMS} analysis of apatite formation in
              extracellular protein matrix of osteoblasts in vitro",
  author   = "Dambach, S and Fartmann, M and Kriegeskotte, C and Br{\"u}ning, C
