@ARTICLE{Arias-Gonzalez2008-sx,
  title    = "Scaling up beta diversity on Caribbean coral reefs",
  author   = "Arias-Gonz{\'a}lez, Jes{\'u}s Ernesto and Legendre, Pierre and
              Rodr{\'\i}guez-Zaragoza, Fabi{\'a}n Alejandro",
  abstract = "The objective of this paper is to find good proxies that are
