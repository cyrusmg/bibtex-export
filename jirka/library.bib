@ARTICLE{Bramanti2013-us,
  title    = "Detrimental effects of ocean acidification on the economically
              important Mediterranean red coral (Corallium rubrum)",
  author   = "Bramanti, Lorenzo and Movilla, J and Guron, M and Calvo, E and
