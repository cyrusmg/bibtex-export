@ARTICLE{Gold2019-nn,
  title    = "The genome of the jellyfish Aurelia and the evolution of animal
              complexity",
  author   = "Gold, David A and Katsuki, Takeo and Li, Yang and Yan, Xifeng and
              Regulski, Michael and Ibberson, David and Holstein, Thomas and
              Steele, Robert E and Jacobs, David K and Greenspan, Ralph J",
  abstract = "We present the genome of the moon jellyfish Aurelia, a genome
