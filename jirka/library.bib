@ARTICLE{Gagnon2012-zn,
  title    = "Seawater transport during coral biomineralization",
  author   = "Gagnon, Alexander C and Adkins, Jess F and Erez, Jonathan",
  abstract = "Cation transport during skeletal growth is a key process
