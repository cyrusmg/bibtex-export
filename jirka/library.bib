  number   =  4,
  pages    = "e34659",
  year     =  2012
}

@ARTICLE{Karako-Lampert2014-qy,
  title    = "Transcriptome Analysis of the Scleractinian Coral Stylophora
              pistillata",
  author   = "Karako-Lampert, Sarit and Zoccola, Didier and Salmon-Divon, Mali
