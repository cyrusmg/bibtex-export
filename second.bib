@ARTICLE{Alvarez-Gallardo2019-wy,
  title     = "High Levels of Physical Fitness Are Associated With Better
               {Health-Related} Quality of Life in Women With Fibromyalgia: The
               al-{\'A}ndalus Project",
  author    = "{\'A}lvarez-Gallardo, Inmaculada C and Soriano-Maldonado,
